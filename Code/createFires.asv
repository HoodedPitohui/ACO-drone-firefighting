function [fires] = createFires()
    t = readtable('../data-manipulation/northeast-samples.xlsx', 'sheet', 'Sheet1')
    t = table2cell(t)
    fireCoords = t(:, 8)
    xCoords = cell2mat(t(:, 15))
    yCoords = cell2mat(t(:, 16))
    fires.intensity = cell2mat(t(:, 8))'; (%[1 1 1 2 2 2 3 3 3 4 4 4 5 5 5];
    fires.locX = [0 0 0 5 5 5 10 10 10 15 15 15 20 20 20];
    fires.locY = [5 10 15 5 10 15 5 10 15 5 10 15 5 10 15];
end
